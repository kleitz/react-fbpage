{
    "parser": "babel-eslint",
    "plugins": [
        "eslint-plugin-react"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "mocha": true
    },
    "globals": {
    },
    "rules": {
        "new-cap": 1,
        "quotes": [2, "single"],
        "eol-last": [0],
        "no-mixed-requires": [0],
        "no-underscore-dangle": [0],
        "no-extra-parens": 0,
        "react/jsx-uses-vars": 1
    },
    "ecmaFeatures": {
        "jsx": true
    }
}